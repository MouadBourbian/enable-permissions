{
  "manifest_version": 3,
  "name": "Enable Permissions",
  "version": "1.0.0",
  "description": "This extension enables right-click, select and copy/paste.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon128.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
